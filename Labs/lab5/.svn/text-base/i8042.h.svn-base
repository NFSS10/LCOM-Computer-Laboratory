#ifndef I8042_H
#define I8042_H

#define KBD_IRQ	1	/**< @brief Keyboard IRQ line */
#define MAKECODE_I 0X01
#define BREAKCODE_I 0X81
#define BIT(n) (0x01<<(n))
#define STAT_REG    0x64
#define KBC_CMD_REG 0x64
